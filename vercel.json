{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "DATABASE_URL": "sqlite:///:memory:"
  },
  "functions": {
    "app.py": {
      "runtime": "python3.9"
    }
  }
}